{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "key",
    "self"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "10218"
      ],
      "pattern": "^(.*)$"
    },
    "key": {
      "$id": "#/properties/key",
      "type": "string",
      "title": "The Key Schema",
      "default": "",
      "examples": [
        "RES-33"
      ],
      "pattern": "^(.*)$"
    },
    "self": {
      "$id": "#/properties/self",
      "type": "string",
      "title": "The Self Schema",
      "default": "",
      "examples": [
        "http://localhost:8080/rest/api/2/issue/10218"
      ],
      "pattern": "^(.*)$"
    }
  }
}